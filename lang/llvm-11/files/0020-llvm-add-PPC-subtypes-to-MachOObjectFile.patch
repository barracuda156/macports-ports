From 4eb2fc043c71ab9e549594d055c255906fe29c29 Mon Sep 17 00:00:00 2001
From: Sergey Fedorov <vital.had@gmail.com>
Date: Sat, 22 Oct 2022 20:23:52 +0800
Subject: [PATCH 20/21] llvm: add PPC subtypes to MachOObjectFile. See:
 https://github.com/iains/LLVM-7-branch/commit/1cb4b516a5cf1419bdc5ab17f323ad61568c8248

---
 llvm/lib/Object/MachOObjectFile.cpp | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/llvm/lib/Object/MachOObjectFile.cpp b/llvm/lib/Object/MachOObjectFile.cpp
index 4d85e6f40ec4..a7b50484ac62 100644
--- llvm_master/lib/Object/MachOObjectFile.cpp
+++ macports_master/lib/Object/MachOObjectFile.cpp
@@ -2706,6 +2706,9 @@ Triple MachOObjectFile::getArchTriple(uint32_t CPUType, uint32_t CPUSubType,
   case MachO::CPU_TYPE_POWERPC:
     switch (CPUSubType & ~MachO::CPU_SUBTYPE_MASK) {
     case MachO::CPU_SUBTYPE_POWERPC_ALL:
+    case MachO::CPU_SUBTYPE_POWERPC_7400:
+    case MachO::CPU_SUBTYPE_POWERPC_7450:
+    case MachO::CPU_SUBTYPE_POWERPC_970:
       if (ArchFlag)
         *ArchFlag = "ppc";
       return Triple("ppc-apple-darwin");
-- 
